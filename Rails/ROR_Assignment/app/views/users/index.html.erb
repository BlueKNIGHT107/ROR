<h1>List of users</h1>


<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email-id</th>
            <th>Age</th>
            <th colspan=3>Action</th>
        </tr>
    </thead>

    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.first_name %></td>
            <td><%= user.last_name %></td>
            <td><%= user.email %></td>
            <td><%= user.age %></td>
            <td><%= link_to "Show", user_path(user)%>
            <td><%= link_to "Edit", edit_user_path(user.id), method: edit_user_path(user)%>
            <td><%= link_to "Delete", user_path(user), method: :delete, data:{turbo_method: :delete}%></td>
        </tr>
        <% end %>
    </tbody>



    
</table>
   
<p>
<%= link_to "Create new user", new_user_path%><br>
<%= link_to "Homepage", root_path %>
</p>
